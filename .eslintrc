{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "plugins": [ "flowtype", "babel" ],
  "globals": {
    "$ReadOnly": true,
    "$ReadOnlyArray": true,
    "$Exact": true
  },
  "rules": {
    "no-unused-vars": ["error", {
      "argsIgnorePattern": "^_"
    }]
  }
}